{
  "name": "react-web-template-project",
  "description": "This is a template with create-react-app, eslint, prettier for to quickly start your project",
  "version": "0.0.1",
  "license": "ISC",
  "private": false,
  "scripts": {
    "dev": "react-scripts start",
    "build": "react-scripts build",
    "dev:build": "yarn build && yarn run serve -s build/",
    "test": "echo \"No tests.\" && exit 0",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.0.1",
    "typeface-roboto": "^0.0.75"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.2",
    "prettier": "^1.18.2",
    "serve": "^11.1.0"
  }
}
